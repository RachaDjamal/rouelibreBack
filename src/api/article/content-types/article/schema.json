{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "writer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::writer.writer",
      "inversedBy": "articles"
    },
    "thumbnail": {
      "type": "component",
      "repeatable": true,
      "component": "shared.images"
    },
    "Keywords": {
      "type": "text"
    }
  }
}
